#include"main.h"
#include<stdio.h>
/**
 * _strdup - Function
 * @str: string
 * Return: string
 */
char *_strdup(char *str)
{
char *string;
int i = 0;
int j = 0;
if (str == NULL)
{
return (NULL);
}
while (str[i] = '\0')
{
i++;
}
string = malloc(sizeof(char) * i);
if (string == NULL)
{
return (NULL);
}
for (j = 0; str[j]; j++)
{
string[j] = str[j];
}
return (string);
}
